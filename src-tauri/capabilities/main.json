{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "main-capability",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"websocket:default",
		"store:allow-get",
		"store:allow-set",
		"store:allow-save",
		"store:allow-load",
		{
			"identifier": "http:default",
			"allow": [
				{
					"url": "https://*:*"
				}
			]
		}
	]
}
